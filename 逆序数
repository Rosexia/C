#include<stdio.h>
#include <math.h>
int f(int n,int b)
{
	int i,j;
	int c=0;
	while(n!=0)
	{
		c=c*10+n%10;
		n=n/10;
	}
	b=c;
	return b;
}
int main()
{
	int n,b;
	scanf("%d",&n);
	b=f(n,b);
	printf("%d",b);
}

//-----------------------------------------------------
/*
输入格式:
输入一个整数（可正可负）。
输出格式
输出它的逆序，注意考虑正负。
输入样例1
1234
输出样例1
4321
输入样例2：
-12340
输出样例2：
-4321
*/
